<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Netko.UploadPage">
	<UserControl.Styles>
		<Style>
			<StyleInclude Source="/GlobalSVGstyle.axaml"></StyleInclude>
		</Style>
	</UserControl.Styles>
	<UserControl.Transitions>
		<Transitions>
			<DoubleTransition Property="Opacity" Duration="0:0:0.2"/>
		</Transitions>
	</UserControl.Transitions>
	<Grid ColumnDefinitions="*" RowDefinitions="Auto, 3, Auto">
		<DockPanel  Grid.Row="0" Grid.Column="0">
			<PathIcon Data="{StaticResource upload}" Height="30" Width="30" Margin="5"/>
			<Label Margin="5" FontSize="25">上传管理</Label>
		</DockPanel>
		<Separator Grid.Row="1" Grid.Column="0" Margin="0" Padding="0"></Separator>
		<Label VerticalAlignment="Center" HorizontalAlignment="Center">Ah oh, This Page Hasn't Finished Yet</Label>
	</Grid>
</UserControl>
