<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Netko.TransferPage">
	<UserControl.Styles>
		<Style>
			<StyleInclude Source="/GlobalSVGstyle.axaml"></StyleInclude>
		</Style>
	</UserControl.Styles>
	<UserControl.Transitions>
		<Transitions>
			<DoubleTransition Property="Opacity" Duration="0:0:0.2"/>
		</Transitions>
	</UserControl.Transitions>
	<Grid ColumnDefinitions="*" RowDefinitions="Auto, 3, *">
		<DockPanel  Grid.Row="0" Grid.Column="0">
			<!-->PathIcon Data="{StaticResource setting}" Height="30" Width="30" Margin="5"/>
			<Label Margin="5" FontSize="25">设置</Label>
		</DockPanel>
		<Separator Grid.Row="1" Grid.Column="0" Margin="0" Padding="0"></Separator-->
			<Label Margin="5" FontSize="20" Padding="10 20 3 3">下载</Label>

		</DockPanel>
		<ScrollViewer Grid.Row="2">
			<StackPanel x:Name="DownloadListContiner" Margin="10 0">
				<Button Click="add_task_test">Add Task</Button>
			</StackPanel>
		</ScrollViewer>


	</Grid>
</UserControl>
