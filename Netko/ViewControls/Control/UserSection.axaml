<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Netko.UserSection"
			 >
	<UserControl.Styles>
		<Style>
			<StyleInclude Source="/GlobalSVGstyle.axaml"></StyleInclude>
		</Style>
	</UserControl.Styles>
	<UserControl.Transitions>
		<Transitions>
			<DoubleTransition Property="Opacity" Duration="0:0:0.2"/>
		</Transitions>
	</UserControl.Transitions>
	<Button>
		<DockPanel Margin="0">
			<PathIcon DockPanel.Dock="Left" Data="{StaticResource add}" Height="10" Width="10"/>
			<Label DockPanel.Dock="Left"  x:Name="WO" Height="10" Width="70" Margin="5 0" Padding="0" FontSize="10">LLLDDDdwad</Label>
			<Button Margin="0" Padding="0" Height="10">x</Button>
		</DockPanel>
		
		
	</Button>
</UserControl>
